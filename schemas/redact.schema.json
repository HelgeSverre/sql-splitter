{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RedactStats",
  "description": "Statistics from redaction operation",
  "type": "object",
  "required": [
    "columns_redacted",
    "rows_redacted",
    "table_stats",
    "tables_processed",
    "warnings"
  ],
  "properties": {
    "columns_redacted": {
      "description": "Number of columns redacted",
      "type": "integer",
      "minimum": 0.0
    },
    "rows_redacted": {
      "description": "Number of rows redacted",
      "type": "integer",
      "minimum": 0.0
    },
    "table_stats": {
      "description": "Per-table statistics",
      "type": "array",
      "items": {
        "$ref": "#/definitions/TableRedactStats"
      }
    },
    "tables_processed": {
      "description": "Number of tables processed",
      "type": "integer",
      "minimum": 0.0
    },
    "warnings": {
      "description": "Warning messages",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "definitions": {
    "TableRedactStats": {
      "description": "Per-table redaction statistics",
      "type": "object",
      "required": ["columns_redacted", "name", "rows_processed"],
      "properties": {
        "columns_redacted": {
          "type": "integer",
          "minimum": 0.0
        },
        "name": {
          "type": "string"
        },
        "rows_processed": {
          "type": "integer",
          "minimum": 0.0
        }
      }
    }
  }
}
