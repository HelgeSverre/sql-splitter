{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "AnalyzeJsonOutput",
  "description": "JSON output for single file analyze",
  "type": "object",
  "required": [
    "dialect",
    "elapsed_secs",
    "input_file",
    "size_mb",
    "summary",
    "tables"
  ],
  "properties": {
    "dialect": {
      "type": "string"
    },
    "elapsed_secs": {
      "type": "number"
    },
    "input_file": {
      "type": "string"
    },
    "size_mb": {
      "type": "number"
    },
    "summary": {
      "$ref": "#/definitions/AnalyzeSummary"
    },
    "tables": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/TableAnalysis"
      }
    }
  },
  "definitions": {
    "AnalyzeSummary": {
      "type": "object",
      "required": ["total_bytes", "total_inserts", "total_tables"],
      "properties": {
        "total_bytes": {
          "type": "integer",
          "minimum": 0.0
        },
        "total_inserts": {
          "type": "integer",
          "minimum": 0.0
        },
        "total_tables": {
          "type": "integer",
          "minimum": 0.0
        }
      }
    },
    "TableAnalysis": {
      "type": "object",
      "required": [
        "bytes",
        "creates",
        "inserts",
        "name",
        "size_mb",
        "statements"
      ],
      "properties": {
        "bytes": {
          "type": "integer",
          "minimum": 0.0
        },
        "creates": {
          "type": "integer",
          "minimum": 0.0
        },
        "inserts": {
          "type": "integer",
          "minimum": 0.0
        },
        "name": {
          "type": "string"
        },
        "size_mb": {
          "type": "number"
        },
        "statements": {
          "type": "integer",
          "minimum": 0.0
        }
      }
    }
  }
}
