{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SampleJsonOutput",
  "description": "JSON output for sample command",
  "type": "object",
  "required": [
    "dialect",
    "dry_run",
    "input_file",
    "mode",
    "statistics",
    "tables"
  ],
  "properties": {
    "dialect": {
      "type": "string"
    },
    "dry_run": {
      "type": "boolean"
    },
    "input_file": {
      "type": "string"
    },
    "mode": {
      "$ref": "#/definitions/SampleModeJson"
    },
    "output_file": {
      "type": ["string", "null"]
    },
    "statistics": {
      "$ref": "#/definitions/SampleStatistics"
    },
    "tables": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/TableSampleJson"
      }
    },
    "warnings": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "definitions": {
    "SampleModeJson": {
      "type": "object",
      "required": ["type", "value"],
      "properties": {
        "type": {
          "type": "string"
        },
        "value": {
          "type": "integer",
          "minimum": 0.0
        }
      }
    },
    "SampleStatistics": {
      "type": "object",
      "required": [
        "fk_orphans_rejected",
        "rows_selected",
        "rows_total",
        "sample_rate_percent",
        "tables_sampled",
        "tables_skipped"
      ],
      "properties": {
        "fk_orphans_rejected": {
          "type": "integer",
          "minimum": 0.0
        },
        "rows_selected": {
          "type": "integer",
          "minimum": 0.0
        },
        "rows_total": {
          "type": "integer",
          "minimum": 0.0
        },
        "sample_rate_percent": {
          "type": "number"
        },
        "tables_sampled": {
          "type": "integer",
          "minimum": 0.0
        },
        "tables_skipped": {
          "type": "integer",
          "minimum": 0.0
        }
      }
    },
    "TableSampleJson": {
      "type": "object",
      "required": [
        "classification",
        "name",
        "rows_selected",
        "rows_total",
        "sample_rate_percent"
      ],
      "properties": {
        "classification": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "rows_selected": {
          "type": "integer",
          "minimum": 0.0
        },
        "rows_total": {
          "type": "integer",
          "minimum": 0.0
        },
        "sample_rate_percent": {
          "type": "number"
        }
      }
    }
  }
}
